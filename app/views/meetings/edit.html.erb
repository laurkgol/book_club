
  <h2>Edit Meeting</h2>
  <ul>
  <%= form_for [@club, @meeting] do |f| %>
    <li><%= f.label :date %></li>
    <li><%= f.text_field :date %></li>

    <li><%= f.label :host %></li>
    <li><%= f.text_field :host %></li>

    <li><%= f.label :location %></li>
    <li><%= f.text_field :location %></li>

    <li><%= f.submit %></li>
  <% end %>

  <%= form_for [@club, @meeting], method: :delete do |f| %>
    <li><%= f.submit "Delete", class: "delete" %></li>
  <% end %>
  </ul>
