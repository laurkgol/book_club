<h2>Edit Member</h2>

<%= form_for [@club, @member] do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :email %>
  <%= f.text_field :email %>

  <%= f.label :street_address %>
  <%= f.text_field :street_address %>

  <%= f.label :street_address_2 %>
  <%= f.text_field :street_address_2 %>

  <%= f.label :city %>
  <%= f.text_field :city %>

  <%= f.label :state %>
  <%= f.text_field :state %>

  <%= f.label :photo_url %>
  <%= f.text_field :photo_url %>


  <%= f.submit %>
<% end %>

<%= form_for [@club, @member], method: :delete do |f| %>
  <%= f.submit "Delete", class: "delete" %>
<% end %>
