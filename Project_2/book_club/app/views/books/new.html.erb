<h2>New Book</h2>

<%= form_for [@club, @book] do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :author %>
  <%= f.text_field :author %>

  <%= f.label :photo_url %>
  <%= f.text_field :photo_url %>

  <%= f.label :link %>
  <%= f.text_field :link %>

  <%= f.label :genre %>
  <%= f.text_field :genre %>

  <%= f.submit %>
<% end %>
